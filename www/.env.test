# .env.test
# Fichier d'environnement pour les tests

APP_ENV=test
APP_SECRET=test_secret_for_unit_tests
APP_DEBUG=1

# Base de données de test (SQLite en mémoire pour les tests rapides)
DATABASE_URL="sqlite:///:memory:"

# API Keys de test (fausses clés)
OPENROUTER_API_KEY=test_api_key_for_unit_tests
APP_URL=https://test.localhost

# Désactiver les logs en test
SHELL_VERBOSITY=-1
